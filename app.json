    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8663e1d8-9c72-4f9f-8485-6f34cc91d5c2"
      }
    },
    "owner": "tog22",
    /* custom additions by me */

    /* â†“ based on https://blog.logrocket.com/css-modules-react-native/ */
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "css"]
    }
    
  }
}
